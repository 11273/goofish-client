{
  "name": "goofish-client",
  "version": "1.2.0",
  "description": "Unofficial Goofish (闲鱼) Web API Client",
  "keywords": [
    "goofish",
    "client",
    "api",
    "sdk",
    "xianyu",
    "taobao",
    "mtop",
    "passport",
    "login",
    "typescript",
    "reverse-engineering"
  ],
  "author": "github.com/11273",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/11273/goofish-client"
  },
  "homepage": "https://11273.github.io/goofish-client",
  "bugs": {
    "url": "https://github.com/11273/goofish-client/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "SECURITY.md",
    "CONTRIBUTING.md",
    "assets",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "example": "tsx examples/index.ts",
    "dev": "tsup --watch",
    "build": "tsup",
    "build:prod": "NODE_ENV=production tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run typecheck && npm run lint && npm run build:prod",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:preview": "release-it --dry-run",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "crypto-js": "^4.2.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "release-it": "^19.0.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitepress": "^1.6.4"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
